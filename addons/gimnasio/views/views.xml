<odoo>
  <data>
  
    <record model="ir.ui.view" id="gym_member_view_list">
      <field name="name">gym.member.list</field>
      <field name="model">gym.member</field>
      <field name="arch" type="xml">
        <list>
          <field name="name"/>
          <field name="dni"/>
          <field name="email"/>
          <field name="plan_id"/>
          <field name="start_date"/>
          <field name="end_date"/>
          <field name="active_membership"/>
        </list>
      </field>
    </record>


    <record model="ir.actions.act_window" id="gym_member_action_window">
      <field name="name">Gimnasio</field>
      <field name="res_model">gym.member</field>
      <field name="view_mode">list,form</field>
    </record>



   <menuitem name="Gimnasio" id="gym_menu_root" web_icon="gym_management,static/description/icon.png"/>

    <menuitem name="Operaciones" id="gym_menu_operations" parent="gym_menu_root"/>

    <menuitem name="Socios" id="gym_menu_member_list" 
              parent="gym_menu_operations" 
              action="gym_member_action_window"/>
  </data>
</odoo>
